	i : INT;
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF (pCar = 0) THEN
				
	pCar := __NEW(Car);
	
END_IF		

FOR i := 1 TO Param.iMaxArray BY 1 DO
	
//	IF (pVehicle = 0) THEN
				
