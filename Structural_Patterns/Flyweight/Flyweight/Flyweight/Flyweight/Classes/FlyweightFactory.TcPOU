		pCar^.setPatent(Patent[i]);	

		pCar^.setOwner(Owner[i]);				
		
 		ADSLOGSTR( msgCtrlMask := ADSLOG_MSGTYPE_HINT, msgFmtStr := 'Data Car: %s', strArg := pCar^.ShowDetails());
		
