FUNCTION_BLOCK ElectricMotor
VAR
	_bConnected : BOOL;
	_bActivated : BOOL;
	_bTraveling	: BOOL;
	
	
END_VAR
</xhtml>
                  </InterfaceAsPlainText>
                </data>
