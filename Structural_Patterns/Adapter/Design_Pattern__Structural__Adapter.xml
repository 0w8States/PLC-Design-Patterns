            <pou name="MAIN" pouType="program">
              <interface>
                <localVars>
                  <variable name="gasolineMotor">
                    <type>
                      <derived name="GasolineMotor" />
                    </type>
                  </variable>
                  <variable name="dieselMotor">
                    <type>
                      <derived name="DieselMotor" />
                    </type>
                  </variable>
                  <variable name="electricAdapterMotor">
