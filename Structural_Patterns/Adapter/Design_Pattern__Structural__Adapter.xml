          <data name="http://www.3s-software.com/plcopenxml/pou" handleUnknown="implementation">
            <pou name="ElectricMotor" pouType="functionBlock">
              <interface>
                <localVars>
                  <variable name="_bConnected">
                    <type>
                      <BOOL />
                    </type>
                  </variable>
                  <variable name="_bActivated">
                    <type>
                      <BOOL />
                    </type>
                  </variable>
                  <variable name="_bTraveling">
                    <type>
                      <BOOL />
                    </type>
                  </variable>
                </localVars>
                <addData>
                  <data name="http://www.3s-software.com/plcopenxml/attributes" handleUnknown="implementation">
                    <Attributes>
                      <Attribute Name="enable_dynamic_creation" Value="" />
                    </Attributes>
                  </data>
                </addData>
              </interface>
              <body>
                <ST>
                  <xhtml xmlns="http://www.w3.org/1999/xhtml" />
                </ST>
              </body>
              <addData>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Activate" ObjectId="919712d6-a70e-4843-ae3b-0422bec14f2d">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF NOT _bConnected THEN
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to activate an electric motor already not connected!',
              	strArg:='');
			  
ELSE
	
	_bActivated := TRUE;
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='electric motor activated!',
              	strArg:='');
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Activate

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Connect" ObjectId="f23918c7-75d6-455d-9062-b465a5eb99ae">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF _bConnected THEN
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to connect an electric motor already connected!',
              	strArg:='');
			  
ELSE
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='electric motor connected!',
              	strArg:='');
				
	_bConnected := TRUE;
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Connect

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Deactivate" ObjectId="00715cc9-b67b-492d-957f-2c319a834874">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF _bActivated THEN
	
	_bActivated := FALSE;
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Electric motor deactivated!',
              	strArg:='');
ELSE
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to deactivated an electric motor already connected!',
              	strArg:='');
				
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Deactivate

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Disconnect" ObjectId="6876e10a-681f-4cfd-9d7a-b8c963cb60ec">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF _bConnected THEN
	
	_bConnected := FALSE;
		
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='electric motor disconnected...',
              	strArg:='');
ELSE
		
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to disconnect an electric motor that is connected!',
              	strArg:='');
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Disconnect

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Plug" ObjectId="bff3f290-2c22-4a2f-ada1-71d8739f308d">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF NOT _bActivated THEN
	
	_bActivated := FALSE;
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Electric motor plugging bateries...',
              	strArg:='');
ELSE
	

	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to plug an electric motor!',
              	strArg:='');
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Plug

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Stop" ObjectId="c9670926-500f-4560-a05c-d67b90954020">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF _bTraveling THEN
	
	_bTraveling := FALSE;

	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Stopping vehicle with electric motor...',
              	strArg:='');
ELSE
	
	_bActivated := TRUE;
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Impossible to stop an electric motor that is not traveling!',
              	strArg:='');
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Stop
</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
                <data name="http://www.3s-software.com/plcopenxml/method" handleUnknown="implementation">
                  <Method name="Travel" ObjectId="88fc0069-7bf0-49f0-836e-e84ce685532f">
                    <interface />
                    <body>
                      <ST>
                        <xhtml xmlns="http://www.w3.org/1999/xhtml">IF (_bConnected AND _bActivated) THEN
	
	_bTraveling := TRUE;
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='Traveling vehicle with electric motor...',
              	strArg:='');
ELSE
	
	ADSLOGSTR(	msgCtrlMask:=ADSLOG_MSGTYPE_LOG,
              	msgFmtStr:='The electric motor must be connected and activated!',
              	strArg:='');
END_IF</xhtml>
                      </ST>
                    </body>
                    <InterfaceAsPlainText>
                      <xhtml xmlns="http://www.w3.org/1999/xhtml">METHOD Travel

</xhtml>
                    </InterfaceAsPlainText>
                    <addData />
                  </Method>
                </data>
